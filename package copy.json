{
  "name": "starsky",
  "type": "module",
  "version": "0.2.3",
  "private": true,
  "packageManager": "pnpm@8.9.0",
  "author": "starknt",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "start": "node .output/server/index.mjs",
    "lint": "eslint --cache .",
    "build": "nuxi build",
    "dev": "nuxi dev",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "postinstall": "concurrently -n githooks,nuxt-prepare,buildpacks \"npx simple-git-hooks\" \"nuxi prepare\" \"pnpm -r --parallel --stream --filter ./packages/* run build\"",
    "typecheck": "nuxi typecheck",
    "release": "bumpp --commit --tag --push",
    "test": "vitest",
    "deploy:node": "cross-env NITRO_PRESET=node_server nuxt build",
    "deploy:cloudflare": "cross-env NITRO_PRESET=cloudflare_module nuxt build"
  },
  "dependencies": {
    "@microsoft/fetch-event-source": "workspace:*",
    "@nanostores/persistent": "^0.9.1",
    "@nanostores/vue": "^0.10.0",
    "@nuxtjs/color-mode": "^3.3.0",
    "@vueuse/core": "^10.5.0",
    "@vueuse/integrations": "^10.5.0",
    "dayjs": "^1.11.10",
    "docx": "^8.2.3",
    "file-saver": "^2.0.5",
    "gpt-tokenizer": "^2.1.2",
    "gsap": "^3.12.2",
    "idb-keyval": "^6.2.1",
    "lodash-es": "^4.17.21",
    "lru-cache": "^10.0.1",
    "markdown-it": "^13.0.2",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-mathjax3": "^4.3.2",
    "naive-ui": "^2.35.0",
    "nanostores": "^0.9.3",
    "page-lifecycle": "^0.1.2",
    "pm2": "^5.3.0",
    "shikiji": "^0.6.10",
    "tsparticles-slim": "^2.12.0",
    "ufo": "^1.3.1",
    "vue-i18n": "^9.5.0",
    "vue3-particles": "^2.12.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.43.1",
    "@css-render/vue3-ssr": "^0.15.12",
    "@iconify/json": "^2.2.127",
    "@intlify/unplugin-vue-i18n": "^1.4.0",
    "@nuxt/devtools": "^0.8.5",
    "@nuxt/kit": "^3.7.4",
    "@types/file-saver": "^2.0.5",
    "@types/markdown-it": "^13.0.2",
    "@types/markdown-it-emoji": "^2.0.2",
    "@unhead/schema": "^1.7.5",
    "@unocss/eslint-config": "^0.56.5",
    "@unocss/nuxt": "^0.56.5",
    "@vicons/ionicons5": "^0.12.0",
    "@vueuse/nuxt": "^10.5.0",
    "bumpp": "^9.2.0",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "happy-dom": "^12.9.1",
    "lint-staged": "^14.0.1",
    "nuxt": "^3.7.4",
    "nuxt-security": "^0.14.4",
    "nuxt-vitest": "^0.11.0",
    "pnpm": "8.9.0",
    "simple-git": "^3.20.0",
    "simple-git-hooks": "^2.9.0",
    "std-env": "^3.4.3",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "unocss": "^0.56.5",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.18"
  },
  "pnpm": {
    "overrides": {
      "deep-equal": "npm:@nolyfill/deep-equal@latest",
      "has": "npm:@nolyfill/has@latest",
      "side-channel": "npm:@nolyfill/side-channel@latest"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
