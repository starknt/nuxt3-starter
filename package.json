{
  "name": "nuxt3-starter",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "deploy:cloudflare": "cross-env NITRO_PRESET=cloudflare_module nuxt build",
    "deploy:node": "cross-env NITRO_PRESET=node_server nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "lint": "eslint --cache .",
    "postinstall": "concurrently -n githooks,nuxt-prepare \"npx simple-git-hooks\" \"nuxi prepare\"",
    "preview": "nuxt preview",
    "release": "bumpp --commit --tag --push",
    "test": "vitest",
    "typecheck": "nuxi typecheck"
  },
  "dependencies": {
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.43.1",
    "@css-render/vue3-ssr": "^0.15.12",
    "@iconify/json": "^2.2.127",
    "@intlify/unplugin-vue-i18n": "^1.4.0",
    "@nuxt/devtools": "latest",
    "@nuxt/kit": "^3.7.4",
    "@nuxtjs/color-mode": "^3.3.0",
    "@unocss/nuxt": "^0.56.5",
    "@vueuse/core": "^10.5.0",
    "@vueuse/integrations": "^10.5.0",
    "@vueuse/nuxt": "^10.5.0",
    "bumpp": "^9.2.0",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "lint-staged": "^14.0.1",
    "naive-ui": "^2.35.0",
    "nuxt": "^3.7.4",
    "nuxt-security": "^0.14.4",
    "nuxt-vitest": "^0.11.0",
    "simple-git": "^3.20.0",
    "simple-git-hooks": "^2.9.0",
    "std-env": "^3.4.3",
    "typescript": "^5.2.2",
    "ufo": "^1.3.1",
    "unocss": "^0.56.5",
    "vitest": "^0.34.6",
    "vue": "^3.3.4",
    "vue-i18n": "^9.5.0",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.18"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
